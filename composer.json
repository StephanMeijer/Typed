{
    "name": "stephanmeijer/typed",
    "version": "0.0.1",
    "description": "Ensure your value's type",
    "type": "library",
    "license": "GPL-3.0",
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "^3.9",
        "vimeo/psalm": "^5.23",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpstan/extension-installer": "^1.3",
        "phpmd/phpmd": "^2.15"
    },
    "autoload": {
        "psr-4": {
            "StephanMeijer\\Typed\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\StephanMeijer\\Typed\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Stephan Meijer",
            "email": "me@stephanmeijer.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
